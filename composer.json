{
    "name": "rolle-marketplace/nager-date-laravel",
    "description": "Laravel wrapper for the Nager.Date API",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "nager",
        "date",
        "holidays",
        "api"
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "RolleMarketplace\\NagerDateLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RolleMarketplace\\NagerDateLaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RolleMarketplace\\NagerDateLaravel\\NagerDateServiceProvider"
            ],
            "aliases": {
                "NagerDate": "RolleMarketplace\\NagerDateLaravel\\Facades\\NagerDate"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}